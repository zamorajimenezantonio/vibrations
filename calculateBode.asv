function [f,mag_vect]=calculateBode(w0,winc,wend)

    A = cell2mat(struct2cell(load('A.mat')));
    B = cell2mat(struct2cell(load('B.mat')));
    C = cell2mat(struct2cell(load('C.mat')));
    D=[[0]];
    
    sys1 = ss(A,B,C,D);
    w=w0:winc:wend;
    [mag,phase,wout] = bode(sys1,w);
    mag_vect=mag(:);
    f=wout/(2*pi);
    %loglog(f,mag_vect);
    
    
    
end

